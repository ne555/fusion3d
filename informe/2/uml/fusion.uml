@startuml
	skinparam monochrome true
	skinparam shadowing false
	hide circle
	hide empty members

	class cloud{}
	class transformation{}

	class fusion{
		merge()
		discard()
		triangulate()
	}
	class mesh{}
	class edge_list{}

	cloud -- transformation
	cloud o- fusion
	mesh -- fusion
	mesh *- edge_list
	mesh *-- cloud

@enduml
