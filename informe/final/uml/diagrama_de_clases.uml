@startuml
	skinparam monochrome true
	skinparam shadowing false
	skinparam defaultFontName lmroman12-regular
	'skinparam dpi 300
	hide circle
	hide empty members

	class Punto{
		posición
		normal
	}

	class Nube{}

	class Registración{
		establecer correspondencias()
		filtrar correspondencias()
		calcular transformación()
	}

	class Transformación{}

	class Anclaje{
		descriptor
		marco de referencia

		calcular transformación(Anclaje)
		distancia(Anclaje)
	}

	class Fusión{
		unir()
		descartar()
		triangular()
	}

	class Malla{}
	class DCEL{}

	class "Relleno de huecos"{
		rellenar()
		dividir()
		unir()
	}
	class Borde{}

	Nube *--> "*" Punto
	Transformación "1" <- Nube
	Registración -> "*" Anclaje
	Registración --> "1" Transformación
	Anclaje --> "1" Nube

	Nube "*" <-o Fusión
	Malla "1" <-- Fusión
	Malla *-> "1" DCEL
	Nube "1" <-* Malla

	"Relleno de huecos" --> "1" Malla
	'Malla "1" - "*" Borde
	DCEL -- "*" Borde
@enduml

